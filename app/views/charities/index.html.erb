

<center><h1>All Charities</h1></center>


<table>

  <thead>
    <tr>
      <th><h2>Name</h2></th>
      <th><h2>Description</h2></th>
      
      
      <th colspan="10"></th>
    </tr>
  </thead>
  

  <tbody>
    <% @charities.each do |charity| %>
      <tr>
        <strong><td><%= link_to(charity.name, charity_path(charity)) %></td></strong>
        
        <td><%= charity.description %></td>
        <td><%= link_to 'Edit', edit_charity_path(charity) %></td>
        <td><%= link_to 'Destroy', charity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

